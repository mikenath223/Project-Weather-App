{
"env": {
    "browser": true,
    "es6": true,
    "jest": true
},
"parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
},
"extends": ["airbnb-base"],
"rules": {
    "no-shadow": "off",
    "no-new": 0,
    "no-param-reassign": "off",
    "eol-last": "off",
    "arrow-parens": "off"
}
}
